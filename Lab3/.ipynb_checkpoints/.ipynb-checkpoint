   ],
   "source": [
    "plt.figure(figsize=(10, 6))\n",
    "\n",
    "plt.plot(x, (1-2*np.log(x))/x**3, label=\"$f^{'}$\")\n",
    "plt.plot(x, (6*np.log(x)-5)/x**4, label=\"$f^{''}$\")\n",
    "\n",
    "plt.legend()\n",
    "plt.xlabel(\"x\")\n",
    "plt.ylabel(\"y\")\n",
    "plt.title(\"Newton Method\")\n",
    "plt.show()"
   ]
  },
